<h1>Upload a Salary and Rankings dataset</h1>
<hr>
<%= form_for @dataset, html: { multipart: true } do |f| %>
  <p>
    <%= f.label :salaries %>
    <%= f.file_field :salaries %>
  </p>
  <p>
    <%= f.label :rankings %>
    <%= f.file_field :rankings %>
  </p>
  <p>
    <%= f.submit "Import new dataset", class: "btn btn-info" %>
  </p>
<% end %>
