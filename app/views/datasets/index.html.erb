<h2>Listing Datasets</h2>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @datasets.each do |dataset| %>
      <tr>
        <td><%= dataset.rankings_filename %></td>
        <td><%= dataset.salaries.filename %></td>
      </tr>
    <% end %>
  </tbody>
</table>

